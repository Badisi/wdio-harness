{
    "name": "@badisi/wdio-harness",
    "version": "1.0.0",
    "description": "WebdriverIO support for Angular component test harnesses.",
    "homepage": "https://github.com/badisi/wdio-harness",
    "license": "MIT",
    "author": {
        "name": "Badisi"
    },
    "main": "cjs/index.js",
    "module": "esm/index.js",
    "types": "esm/index.d.ts",
    "exports": {
        ".": {
            "require": "./cjs/index.js",
            "import": "./esm/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/badisi/wdio-harness.git"
    },
    "keywords": [
        "angular",
        "cdk",
        "components",
        "test",
        "testing",
        "harness",
        "harnesses",
        "test-harnesses",
        "wdio",
        "webdriver",
        "webdriverio"
    ],
    "scripts": {
        "start": "ng serve",
        "test": "npm run start --prefix=projects/tests -- --debug=true --headless=false --watch=true",
        "build": "node ./projects/library/build.js"
    },
    "peerDependencies": {
        "@angular/cdk": ">= 10 < 13",
        "@wdio/logger": ">= 7 < 8",
        "colors": "^1.4.0",
        "webdriverio": ">= 7 < 8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~12.2.13",
        "@angular/animations": "~12.2.0",
        "@angular/cdk": "^12.2.13",
        "@angular/cli": "~12.2.13",
        "@angular/common": "~12.2.0",
        "@angular/compiler": "~12.2.0",
        "@angular/compiler-cli": "~12.2.0",
        "@angular/core": "~12.2.0",
        "@angular/forms": "~12.2.0",
        "@angular/material": "^12.2.13",
        "@angular/platform-browser": "~12.2.0",
        "@angular/platform-browser-dynamic": "~12.2.0",
        "@angular/router": "~12.2.0",
        "@types/jasmine": "^3.10.2",
        "@types/node": "^16.11.7",
        "@wdio/cli": "^7.16.5",
        "@wdio/jasmine-framework": "^7.16.5",
        "@wdio/local-runner": "^7.16.5",
        "@wdio/logger": "^7.16.0",
        "@wdio/spec-reporter": "^7.16.4",
        "colors": "^1.4.0",
        "cpy": "^8.1.2",
        "jasmine-core": "^3.10.1",
        "rxjs": "~6.6.0",
        "ts-node": "^10.4.0",
        "tsconfig-paths": "^3.11.0",
        "tslib": "^2.3.0",
        "typescript": "~4.3.5",
        "wdio-chromedriver-service": "^7.2.2",
        "zone.js": "~0.11.4"
    }
}
