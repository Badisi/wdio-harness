{
    "name": "@badisi/wdio-harness",
    "description": "WebdriverIO support for Angular component test harnesses.",
    "homepage": "https://github.com/badisi/wdio-harness",
    "license": "MIT",
    "author": {
        "name": "Badisi"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/badisi/wdio-harness.git"
    },
    "keywords": [
        "angular",
        "cdk",
        "components",
        "test",
        "testing",
        "harness",
        "harnesses",
        "test-harnesses",
        "wdio",
        "webdriver",
        "webdriverio"
    ],
    "private": true,
    "scripts": {
        "ncu": "npx ncu -i --format=group --packageFile '{,projects/**/}package.json'",
        "start:tests-app": "ng serve",
        "start:tests-e2e": "npm run start --prefix=projects/tests-e2e",
        "build:lib": "npm run build --prefix=projects/library",
        "build:tests-app": "ng build"
    },
    "workspaces": [
        "projects/library",
        "projects/tests-e2e"
    ],
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.10",
        "@angular/animations": "~13.3.12",
        "@angular/cdk": "^13.3.9",
        "@angular/cli": "~13.3.10",
        "@angular/common": "~13.3.12",
        "@angular/compiler": "~13.3.12",
        "@angular/compiler-cli": "~13.3.12",
        "@angular/core": "~13.3.12",
        "@angular/forms": "~13.3.12",
        "@angular/material": "^13.3.9",
        "@angular/platform-browser": "~13.3.12",
        "@angular/platform-browser-dynamic": "~13.3.12",
        "@angular/router": "~13.3.12",
        "@types/jasmine": "^4.3.1",
        "@types/node": "^18.11.15",
        "jasmine-core": "^4.5.0",
        "rxjs": "^7.5.6",
        "ts-node": "^10.9.1",
        "tslib": "^2.4.1",
        "typescript": "^4.6.4",
        "zone.js": "^0.11.7"
    }
}
